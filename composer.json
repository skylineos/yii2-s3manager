{
    "name": "skylineos/yii2-s3manager",
    "type": "yii2-extension",
    "description": "A yii2 extension for managing files in AWS S3 buckets",
    "keywords": [
        "yii2",
        "aws",
        "s3",
        "image manager",
        "file manager",
        "widget"
    ],
    "license": "GPL-3.0",
    "support": {
        "issues": "https://github.com/skylineos/yii2-s3manager/issues",
        "source": "https://github.com/skylineos/yii2-s3manager"
    },
    "authors": [
        {
            "name": "Damien Kemens",
            "email": "damien.kemens@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-imagine": "*",
        "aws/aws-sdk-php": "^3.64",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "thiagotalma/yii2-jstree": "~1.0.0",
        "league/flysystem": "^2.0",
        "league/flysystem-aws-s3-v3": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "skylineos\\yii\\s3manager\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}